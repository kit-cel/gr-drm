<?xml version="1.0"?>
<block>
  <name>DRM Partitioning 16-QAM EEP</name>
  <key>drm_partitioning_16_bb</key>
  <category>DRM</category>
  <import>import drm</import>
  <make>drm.partitioning_bb($vlen_in, $vlen_out)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Input vector length</name>
    <key>vlen_in</key>
    <type>int</type>
    <hide>part</hide>
  </param>
  <param>
    <name>Output vector lengths</name>
    <key>vlen_out</key>
    <type>raw</type>
    <hide>part</hide>
  </param>
  <param>
    <name>Length of topmost output</name>
    <key>vlen1</key>
    <type>int</type>
    <hide>part</hide>
  </param>
  <param>
    <name>Length of second output</name>
    <key>vlen2</key>
    <type>int</type>
    <hide>part</hide>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>byte</type>    
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out1</name>
    <type>byte</type>    
  </source>
  <source>
    <name>out2</name>
    <type>byte</type>    
  </source>
</block>
